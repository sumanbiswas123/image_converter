{
 "$schema": "https://schema.tauri.app/config/2",
 "productName": "image-converter-tauri",
 "version": "0.1.0",
 "identifier": "com.image-converter-taurai.app",
 "build": {
   "beforeDevCommand": "npm run dev",
   "devUrl": "http://localhost:1420",
   "beforeBuildCommand": "npm run build",
   "frontendDist": "../dist"
 },
 "app": {
   "windows": [
     {
       "title": "image-converter-tauri",
       "width": 1280,
       "height": 800
     }
   ],
   "security": {
     "csp": null
   }
 },
 "bundle": {
   "active": false,
   "targets": "all",
   "icon": [
     "icons/32x32.png",
     "icons/128x128.png",
     "icons/128x128@2x.png",
     "icons/icon.icns",
     "icons/icon.ico"
   ]
 }
}